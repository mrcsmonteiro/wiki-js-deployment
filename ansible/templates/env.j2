# Wiki.js Environment Variables
DB_TYPE=postgres
DB_HOST=db
DB_PORT=5432
DB_USER={{ db_user }}
DB_PASS={{ db_password }}
DB_NAME={{ db_name }}
DB_SSL=false
APP_PORT={{ wikijs_port }}

# Other Wiki.js configuration (optional, can be added as needed)
# For example, if you want to set the base URL
URL=https://{{ public_domain }} # Uncommented and set to public_domain
